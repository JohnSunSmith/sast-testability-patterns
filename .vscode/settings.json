{
  "json.schemas": [
    {
      "fileMatch": [
        "JAVA/*/*.json",
        "JS/*/*.json",
        "PHP/*/*.json"
      ],
      "url": "./pattern_template/schema/pattern.schema.json"
    },
    {
      "fileMatch": [
        "JAVA/*/*/*_instance_*.json",
